{
  "root": {
    "npm": "./",
    "dev": "<%= devFolderPath %>",
    "dist": "<%= distFolderPath %>"
  },
  "javascript": {
    "dev": "js",
    "dist": "assets/js",
    "extensions": [".json", ".js"]
  },
  "styles": {
    "dev": "scss/**/*",
    "dist": "assets/css",
    "extensions": ".scss"
  },
  "images": {
    "dev": "img/**/*",
    "dist": "assets/img",
    "extensions": ".{jpg,png,svg,gif}"
  }<% if (hasStyleguide) { %>,
  "styleguide": {
    "dev": "styleguide",
    "dist": "assets/styleguide",
    "extensions": ".{html,json,md,yml}"
  }<% } %>
}
